{
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}",
            "${workspaceFolder}/include",
            "${workspaceFolder}/**",
            "/usr/lib/gcc/x86_64-pc-linux-gnu/*/*",
            "/usr/include/c++/*",
            "/usr/include/*"
        ],
        "myCompilerPath": "/usr/local/bin/gcc"
    },
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "/usr/include/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "compilerArgs": [
                "-Wall"
            ]
        }
    ],
    "version": 4
}